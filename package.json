{
  "name": "sistema-medicamentos-dhs",
  "version": "2.0.0",
  "description": "Sistema DHS via PGS Medicamentos - Plataforma integrada para orientação de acesso a medicamentos no Brasil",
  "private": false,
  "homepage": "https://sistema-medicamentos-dhs.netlify.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/cordeirotelecom/sistema-medicamentos.git"
  },
  "bugs": {
    "url": "https://github.com/cordeirotelecom/sistema-medicamentos/issues"
  },
  "author": {
    "name": "Ministério Público & Defensoria Pública",
    "email": "contato@sistema-medicamentos.gov.br",
    "url": "https://sistema-medicamentos-dhs.netlify.app"
  },
  "contributors": [
    {
      "name": "CORDEIRO Telecom",
      "email": "dev@cordeirotelecom.com",
      "url": "https://github.com/cordeirotelecom"
    }
  ],
  "keywords": [
    "medicamentos",
    "anvisa",
    "ministerio-publico",
    "defensoria-publica",
    "saude-publica",
    "juridico",
    "brasil",
    "next.js",
    "typescript",
    "dhs",
    "pgs",
    "nmc"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "test": "echo \"Tests not configured yet\" && exit 0",
    "export": "next export",
    "analyze": "cross-env ANALYZE=true next build",
    "clean": "rm -rf .next out",
    "prepare": "husky install",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "axios": "^1.11.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.541.0",
    "next": "15.5.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "recharts": "^3.1.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.19.11",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
